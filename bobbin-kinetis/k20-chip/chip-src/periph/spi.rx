        (field
            (name TXDATA)
            (bit-offset 0)
            (bit-width 32)
            (access read-write)
            (description "Transmit Data")
        )
    )
    (register
        (name POPR)
        (offset 0x38)
        (size 0x20)
        (access read-only)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "POP RX FIFO Register")
