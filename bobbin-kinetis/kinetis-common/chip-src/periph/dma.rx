            (description "Destination Offset Error")
            (value
                (value "#0")
                (name "0")
                (description "No destination offset configuration error")
            )
            (value
                (value "#1")
                (name "1")
                (description "The last recorded error was a configuration error detected in the TCDn_DOFF field. TCDn_DOFF is inconsistent with TCDn_ATTR[DSIZE].")
            )
