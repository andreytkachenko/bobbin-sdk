            (bit-offset 8)
            (bit-width 4)
            (access read-only)
            (description "Error Channel Number or Canceled Channel Number")
        )
        (field
            (name CPE)
            (bit-offset 14)
            (bit-width 1)
            (access read-only)
            (description "Channel Priority Error")
            (value
                (value "#0")
                (name "0")
                (description "No channel priority error")
            )
            (value
                (value "#1")
                (name "1")
                (description "The last recorded error was a configuration error in the channel priorities . Channel priorities are not unique.")
            )
        )
        (field
