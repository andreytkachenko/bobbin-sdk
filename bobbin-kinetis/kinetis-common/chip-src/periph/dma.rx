            (bit-offset 16)
            (bit-width 1)
            (access read-only)
            (description "Transfer Canceled")
            (value
                (value "#0")
                (name "0")
                (description "No canceled transfers")
            )
            (value
                (value "#1")
                (name "1")
                (description "The last recorded entry was a canceled transfer by the error cancel transfer input")
            )
        )
