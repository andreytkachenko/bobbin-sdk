            )
        )
        (field
            (name NCE)
            (bit-offset 3)
            (bit-width 1)
            (access read-only)
            (description "NBYTES/CITER Configuration Error")
            (value
                (value "#0")
                (name "0")
                (description "No NBYTES/CITER configuration error")
            )
            (value
                (value "#1")
                (name "1")
                (description "The last recorded error was a configuration error detected in the TCDn_NBYTES or TCDn_CITER fields. TCDn_NBYTES is not a multiple of TCDn_ATTR[SSIZE] and TCDn_ATTR[DSIZE], or TCDn_CITER[CITER] is equal to zero, or TCDn_CITER[ELINK] is not equal to TCDn_BITER[ELINK]")
            )
        )
        (field
            (name DOE)
            (bit-offset 4)
            (bit-width 1)
