(peripheral
    (group-name GPIO)
    (register
        (name PDOR)
        (offset 0x0)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Data Output Register")
        (field
            (name PDO)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Port Data Output")
            (value
                (value "#0")
                (name "0")
                (description "Logic level 0 is driven on pin, provided pin is configured for general-purpose output.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Logic level 1 is driven on pin, provided pin is configured for general-purpose output.")
            )
        )
    )
    (register
        (name PSOR)
        (offset 0x4)
        (size 0x20)
        (access write-only)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Set Output Register")
        (field
            (name PTSO)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access write-only)
            (description "Port Set Output")
            (value
                (value "#0")
                (name "0")
                (description "Corresponding bit in PDORn does not change.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Corresponding bit in PDORn is set to logic 1.")
            )
        )
    )
    (register
        (name PCOR)
        (offset 0x8)
        (size 0x20)
        (access write-only)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Clear Output Register")
        (field
            (name PTCO)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access write-only)
            (description "Port Clear Output")
            (value
                (value "#0")
                (name "0")
                (description "Corresponding bit in PDORn does not change.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Corresponding bit in PDORn is cleared to logic 0.")
            )
        )
    )
    (register
        (name PTOR)
        (offset 0xc)
        (size 0x20)
        (access write-only)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Toggle Output Register")
        (field
            (name PTTO)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access write-only)
            (description "Port Toggle Output")
            (value
                (value "#0")
                (name "0")
                (description "Corresponding bit in PDORn does not change.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Corresponding bit in PDORn is set to the inverse of its existing logic state.")
            )
        )
    )
    (register
        (name PDIR)
        (offset 0x10)
        (size 0x20)
        (access read-only)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Data Input Register")
        (field
            (name PDI)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access read-only)
            (description "Port Data Input")
            (value
                (value "#0")
                (name "0")
                (description "Pin logic level is logic 0, or is not configured for use by digital function.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Pin logic level is logic 1.")
            )
        )
    )
    (register
        (name PDDR)
        (offset 0x14)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Data Direction Register")
        (field
            (name PDD)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Port Data Direction")
            (value
                (value "#0")
                (name "0")
                (description "Pin is configured as general-purpose input, for the GPIO function.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Pin is configured as general-purpose output, for the GPIO function.")
            )
        )
    )
    (register
        ; S32K Only
        (name PIDR)
        (offset 0x18)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Port Input Disable Register")
        (field
            (name PID)
            (dim 32)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Port Input Disable")
        )
    )    
)