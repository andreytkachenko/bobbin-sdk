            (name LPBOOT1)
            (bit-offset 4)
            (bit-width 1)
            (access read-only)
            (description "no description available")
            (value
                (value "#0")
                (name "00")
                (description "Core and system clock divider (OUTDIV1) is 0x7 (divide by 8) when LPBOOT0=0 or 0x3 (divide by 4) when LPBOOT0=1.")
