            (bit-offset 2)
            (bit-width 1)
            (access read-only)
            (description "no description available")
            (value
                (value "#0")
                (name "00")
                (description "NMI interrupts are always blocked")
            )
            (value
                (value "#1")
                (name "01")
                (description "NMI_b pin/interrupts reset default to enabled")
            )
        )
        (field
            (name RESET_PIN_CFG)
            (bit-offset 3)
            (bit-width 1)
            (access read-only)
            (description "no description available")
            (value
                (value "#0")
                (name "00")
                (description "RESET pin is disabled following a POR and cannot be enabled as reset function")
            )
            (value
                (value "#1")
                (name "01")
                (description "RESET_b pin is dedicated")
            )
        )
