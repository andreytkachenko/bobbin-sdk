        (field
            (name VLCDEN)
            (bit-offset 24)
            (bit-width 1)
            (description "VLCD enable")
        )
        (field
            (name LFMEN)
            (bit-offset 25)
            (bit-width 1)
            (description "Low Frequency Mode enable")
