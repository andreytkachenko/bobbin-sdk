        (field
            (name ADSTP)
            (bit-offset 4)
            (bit-width 1)
            (description "ADC stop conversion command")
        )
        (field
            (name ADVREGEN)
            (bit-offset 28)
            (bit-width 1)
            (description "ADC Voltage Regulator Enable")
        )
        (field
            (name ADCAL)
            (bit-offset 31)
            (bit-width 1)
            (description "ADC calibration")
