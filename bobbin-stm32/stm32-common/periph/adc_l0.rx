            (bit-offset 0)
            (bit-width 12)
            (description "DATA (12 bit)")
        )        
        (field
            (name DATA_10)
            (bit-offset 0)
            (bit-width 10)
            (description "DATA (10 bit)")
        )        
        (field
            (name DATA_8)
            (bit-offset 0)
            (bit-width 8)
            (description "DATA (8 bit)")
        )       
        (field
            (name DATA_6)
            (bit-offset 0)
            (bit-width 6)
