            (name TAR)
            (bit-offset 0)
            (bit-width 32)
            (access read-write)
            (description "Time Alarm Register")
        )
    )
    (register
        (name TCR)
        (offset 0xc)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "RTC Time Compensation Register")
        (field
            (name TCR)
            (bit-offset 0)
            (bit-width 8)
            (access read-write)
            (description "Time Compensation Register")
            (value
                (value "#10000000")
                (name "10000000")
                (description "Time Prescaler Register overflows every 32896 clock cycles.")
            )
            (value
                (value "#10000001")
                (name "10000001")
                (description "Time Prescaler Register overflows every 32895 clock cycles.")
            )
            (value
                (value "#11111111")
                (name "11111111")
                (description "Time Prescaler Register overflows every 32769 clock cycles.")
            )
            (value
                (value "#0")
                (name "00000000")
                (description "Time Prescaler Register overflows every 32768 clock cycles.")
            )
            (value
                (value "#1")
                (name "00000001")
                (description "Time Prescaler Register overflows every 32767 clock cycles.")
            )
            (value
                (value "#1111110")
                (name "01111110")
                (description "Time Prescaler Register overflows every 32642 clock cycles.")
            )
            (value
                (value "#1111111")
                (name "01111111")
                (description "Time Prescaler Register overflows every 32641 clock cycles.")
            )
        )
        (field
            (name CIR)
            (bit-offset 8)
            (bit-width 8)
            (access read-write)
            (description "Compensation Interval Register")
        )
        (field
            (name TCV)
            (bit-offset 16)
            (bit-width 8)
            (access read-only)
            (description "Time Compensation Value")
        )
        (field
            (name CIC)
            (bit-offset 24)
            (bit-width 8)
            (access read-only)
            (description "Compensation Interval Counter")
        )
    )
    (register
        (name CR)
        (offset 0x10)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "RTC Control Register")
        (field
            (name SWR)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Software Reset")
            (value
                (value "#0")
                (name "0")
                (description "No effect.")
            )
        )
        (field
            (name SUP)
            (bit-offset 2)
            (bit-width 1)
            (access read-write)
            (description "Supervisor Access")
            (value
                (value "#0")
                (name "0")
                (description "Non-supervisor mode write accesses are not supported and generate a bus error.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Non-supervisor mode write accesses are supported.")
            )
        )
        (field
            (name UM)
            (bit-offset 3)
            (bit-width 1)
            (access read-write)
            (description "Update Mode")
            (value
                (value "#0")
                (name "0")
                (description "Registers cannot be written when locked.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Registers can be written when locked under limited conditions.")
            )
        )
        (field
            (name CPS)
            (bit-offset 5)
            (bit-width 1)
            (access read-write)
            (description "Clock Pin Select")
            (value
                (value "#0")
                (name "0")
                (description "The prescaler output clock (as configured by TSIC) is output on RTC_CLKOUT.")
            )
            (value
                (value "#1")
                (name "1")
                (description "The RTC 32kHz crystal clock is output on RTC_CLKOUT, provided it is output to other peripherals.")
            )
        )
        (field
            (name LPOS)
            (bit-offset 7)
            (bit-width 1)
            (access read-write)
            (description "LPO Select")
            (value
                (value "#0")
                (name "0")
                (description "RTC prescaler increments using 32kHz crystal.")
            )
            (value
                (value "#1")
                (name "1")
                (description "RTC prescaler increments using 1kHz LPO, bits [4:0] of the prescaler are ignored.")
            )
        )
        (field
            (name CPE)
            (bit-offset 24)
            (bit-width 1)
            (access read-write)
            (description "Clock Pin Enable")
            (value
                (value "#0")
                (name "0")
                (description "Disable RTC_CLKOUT pin.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Enable RTC_CLKOUT pin.")
            )
        )
    )
    (register
        (name SR)
        (offset 0x14)
        (size 0x20)
        (access read-write)
        (reset-value 0x1)
        (reset-mask 0xffffffff)
        (description "RTC Status Register")
