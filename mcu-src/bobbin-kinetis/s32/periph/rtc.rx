            (name TIIE)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Time Invalid Interrupt Enable")
            (value
                (value "#0")
                (name "0")
                (description "Time invalid flag does not generate an interrupt.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time invalid flag does generate an interrupt.")
            )
        )
        (field
            (name TOIE)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "Time Overflow Interrupt Enable")
            (value
                (value "#0")
                (name "0")
                (description "Time overflow flag does not generate an interrupt.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time overflow flag does generate an interrupt.")
            )
        )
        (field
            (name TAIE)
            (bit-offset 2)
            (bit-width 1)
            (access read-write)
            (description "Time Alarm Interrupt Enable")
            (value
                (value "#0")
                (name "0")
                (description "Time alarm flag does not generate an interrupt.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time alarm flag does generate an interrupt.")
            )
        )
        (field
            (name TSIE)
            (bit-offset 4)
            (bit-width 1)
            (access read-write)
            (description "Time Seconds Interrupt Enable")
            (value
                (value "#0")
                (name "0")
                (description "Seconds interrupt is disabled.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Seconds interrupt is enabled.")
            )
        )
        (field
            (name TSIC)
            (bit-offset 16)
            (bit-width 3)
            (access read-write)
            (description "Timer Seconds Interrupt Configuration")
            (value
                (value "#000")
                (name "000")
                (description "1 Hz.")
            )
            (value
                (value "#001")
                (name "001")
                (description "2 Hz.")
            )
            (value
                (value "#010")
                (name "010")
                (description "4 Hz.")
            )
            (value
                (value "#011")
                (name "011")
                (description "8 Hz.")
            )
            (value
                (value "#100")
                (name "100")
                (description "16 Hz.")
            )
            (value
                (value "#101")
                (name "101")
                (description "32 Hz.")
            )
            (value
                (value "#110")
                (name "110")
                (description "64 Hz.")
            )
            (value
                (value "#111")
                (name "111")
                (description "128 Hz.")
            )
        )
    )
)