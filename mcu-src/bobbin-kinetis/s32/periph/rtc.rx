        (offset 0x0)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "RTC Time Seconds Register")
        (field
            (name TSR)
            (bit-offset 0)
            (bit-width 32)
            (access read-write)
            (description "Time Seconds Register")
        )
    )
    (register
        (name TPR)
        (offset 0x4)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "RTC Time Prescaler Register")
        (field
            (name TPR)
            (bit-offset 0)
            (bit-width 16)
            (access read-write)
            (description "Time Prescaler Register")
        )
    )
    (register
        (name TAR)
        (offset 0x8)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "RTC Time Alarm Register")
