                (value "#0")
                (name "0")
                (description "Time is valid.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time is invalid and time counter is read as zero.")
            )
        )
        (field
            (name TOF)
            (bit-offset 1)
            (bit-width 1)
            (access read-only)
            (description "Time Overflow Flag")
            (value
                (value "#0")
                (name "0")
                (description "Time overflow has not occurred.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time overflow has occurred and time counter is read as zero.")
            )
        )
        (field
            (name TAF)
            (bit-offset 2)
            (bit-width 1)
            (access read-only)
            (description "Time Alarm Flag")
            (value
                (value "#0")
                (name "0")
                (description "Time alarm has not occurred.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time alarm has occurred.")
            )
        )
        (field
            (name TCE)
            (bit-offset 4)
            (bit-width 1)
            (access read-write)
            (description "Time Counter Enable")
            (value
                (value "#0")
                (name "0")
                (description "Time counter is disabled.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time counter is enabled.")
            )
        )
    )
    (register
        (name LR)
        (offset 0x18)
        (size 0x20)
        (access read-write)
        (reset-value 0xff)
        (reset-mask 0xffffffff)
        (description "RTC Lock Register")
        (field
            (name TCL)
            (bit-offset 3)
            (bit-width 1)
            (access read-write)
            (description "Time Compensation Lock")
            (value
                (value "#0")
                (name "0")
                (description "Time Compensation Register is locked and writes are ignored.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Time Compensation Register is not locked and writes complete as normal.")
            )
        )
        (field
            (name CRL)
            (bit-offset 4)
            (bit-width 1)
            (access read-write)
            (description "Control Register Lock")
            (value
                (value "#0")
                (name "0")
                (description "Control Register is locked and writes are ignored.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Control Register is not locked and writes complete as normal.")
            )
        )
        (field
            (name SRL)
            (bit-offset 5)
            (bit-width 1)
            (access read-write)
            (description "Status Register Lock")
            (value
                (value "#0")
                (name "0")
                (description "Status Register is locked and writes are ignored.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Status Register is not locked and writes complete as normal.")
            )
        )
        (field
            (name LRL)
            (bit-offset 6)
            (bit-width 1)
            (access read-write)
            (description "Lock Register Lock")
            (value
                (value "#0")
                (name "0")
                (description "Lock Register is locked and writes are ignored.")
            )
            (value
                (value "#1")
                (name "1")
                (description "Lock Register is not locked and writes complete as normal.")
            )
        )
    )
    (register
        (name IER)
        (offset 0x1c)
        (size 0x20)
        (access read-write)
        (reset-value 0x7)
        (reset-mask 0xffffffff)
        (description "RTC Interrupt Enable Register")
