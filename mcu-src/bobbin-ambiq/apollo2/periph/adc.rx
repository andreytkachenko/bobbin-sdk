            )
            (value
                (value "2")
                (name "AVG_4_MSRMTS")
                (description "Average in 4 measurements in the accumulate divide module for this slot. value.")
            )
            (value
                (value "3")
                (name "AVG_8_MSRMT")
                (description "Average in 8 measurements in the accumulate divide module for this slot. value.")
            )
            (value
                (value "4")
                (name "AVG_16_MSRMTS")
                (description "Average in 16 measurements in the accumulate divide module for this slot. value.")
            )
            (value
                (value "5")
                (name "AVG_32_MSRMTS")
                (description "Average in 32 measurements in the accumulate divide module for this slot. value.")
            )
            (value
                (value "6")
                (name "AVG_64_MSRMTS")
                (description "Average in 64 measurements in the accumulate divide module for this slot. value.")
            )
            (value
                (value "7")
                (name "AVG_128_MSRMTS")
                (description "Average in 128 measurements in the accumulate divide module for this slot. value.")
            )
        )
        (field
            (name PRMODE)
            (bit-offset 16)
            (bit-width 2)
            (access read-write)
            (description "Set the Precision Mode For Slot.")
            (value
                (value "0")
                (name "P14B")
                (description "14-bit precision mode value.")
            )
            (value
                (value "1")
                (name "P12B")
                (description "12-bit precision mode value.")
            )
            (value
                (value "2")
                (name "P10B")
                (description "10-bit precision mode value.")
            )
            (value
                (value "3")
                (name "P8B")
                (description "8-bit precision mode value.")
            )
        )
        (field
            (name CHSEL)
            (bit-offset 8)
            (bit-width 4)
            (access read-write)
            (description "Select one of the 14 channel inputs for this slot.")
            (value
                (value "0")
                (name "SE0")
                (description "single ended external GPIO connection to pad16. value.")
            )
            (value
                (value "1")
                (name "SE1")
                (description "single ended external GPIO connection to pad29. value.")
            )
            (value
                (value "2")
                (name "SE2")
                (description "single ended external GPIO connection to pad11. value.")
            )
            (value
                (value "3")
                (name "SE3")
                (description "single ended external GPIO connection to pad31. value.")
            )
            (value
                (value "4")
                (name "SE4")
                (description "single ended external GPIO connection to pad32. value.")
            )
            (value
                (value "5")
                (name "SE5")
                (description "single ended external GPIO connection to pad33. value.")
            )
            (value
                (value "6")
                (name "SE6")
                (description "single ended external GPIO connection to pad34. value.")
            )
            (value
                (value "7")
                (name "SE7")
                (description "single ended external GPIO connection to pad35. value.")
            )
            (value
                (value "8")
                (name "SE8")
                (description "single ended external GPIO connection to pad13. value.")
            )
            (value
                (value "9")
                (name "SE9")
                (description "single ended external GPIO connection to pad12. value.")
            )
            (value
                (value "10")
                (name "DF0")
                (description "differential external GPIO connections to pad12(N) and pad13(P). value.")
            )
            (value
                (value "11")
                (name "DF1")
                (description "differential external GPIO connections to pad15(N) and pad14(P). value.")
            )
            (value
                (value "12")
                (name "TEMP")
                (description "internal temperature sensor. value.")
            )
            (value
                (value "13")
                (name "BATT")
                (description "internal voltage divide-by-3 connection. value.")
            )
            (value
                (value "14")
                (name "VSS")
                (description "Input VSS value.")
            )
        )
        (field
            (name WCEN)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "This bit enables the window compare function for slot 0.")
            (value
                (value "1")
                (name "WCEN")
                (description "Enable the window compare for slot 0. value.")
            )
        )
        (field
            (name SLEN)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit enables slot 0 for ADC conversions.")
            (value
                (value "1")
                (name "SLEN")
                (description "Enable slot 0 for ADC conversions. value.")
            )
        )
    )
