(peripheral
    (group-name ADC)
    (size 0x20)
    (access read-write)
    (description "Analog Digital Converter Control")
    (register
        (name CFG)
        (offset 0x0)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x30f031d)
        (description "Configuration Register")
        (field
            (name CLKSEL)
            (bit-offset 24)
            (bit-width 2)
            (access read-write)
            (description "Select the source and frequency for the ADC clock. All values not enumerated below are undefined.")
            (value
                (value "0")
                (name "OFF")
                (description "Off mode. The HFRC or HFRC_DIV2 clock must be selected for the ADC to function. The ADC controller automatically shuts off the clock in it\'s low power modes. When setting ADCEN to \'0\', the CLKSEL should remain set to one of the two clock selects for proper power down sequencing. value.")
            )
            (value
                (value "1")
                (name "HFRC")
                (description "HFRC Core Clock Frequency value.")
            )
            (value
                (value "2")
                (name "HFRC_DIV2")
                (description "HFRC Core Clock / 2 value.")
            )
        )
        (field
            (name TRIGPOL)
            (bit-offset 19)
            (bit-width 1)
            (access read-write)
            (description "This bit selects the ADC trigger polarity for external off chip triggers.")
            (value
                (value "0")
                (name "RISING_EDGE")
                (description "Trigger on rising edge. value.")
            )
            (value
                (value "1")
                (name "FALLING_EDGE")
                (description "Trigger on falling edge. value.")
            )
        )
        (field
            (name TRIGSEL)
            (bit-offset 16)
            (bit-width 3)
            (access read-write)
            (description "Select the ADC trigger source.")
            (value
                (value "0")
                (name "EXT0")
                (description "Off chip External Trigger0 (ADC_ET0) value.")
            )
            (value
                (value "1")
                (name "EXT1")
                (description "Off chip External Trigger1 (ADC_ET1) value.")
            )
            (value
                (value "2")
                (name "EXT2")
                (description "Off chip External Trigger2 (ADC_ET2) value.")
            )
            (value
                (value "3")
                (name "EXT3")
                (description "Off chip External Trigger3 (ADC_ET3) value.")
            )
            (value
                (value "4")
                (name "VCOMP")
                (description "Voltage Comparator Output value.")
            )
            (value
                (value "7")
                (name "SWT")
                (description "Software Trigger value.")
            )
        )
        (field
            (name REFSEL)
            (bit-offset 8)
            (bit-width 2)
            (access read-write)
            (description "Select the ADC reference voltage.")
            (value
                (value "0")
                (name "INT2P0")
                (description "Internal 2.0V Bandgap Reference Voltage value.")
            )
            (value
                (value "1")
                (name "INT1P5")
                (description "Internal 1.5V Bandgap Reference Voltage value.")
            )
            (value
                (value "2")
                (name "EXT2P0")
                (description "Off Chip 2.0V Reference value.")
            )
            (value
                (value "3")
                (name "EXT1P5")
                (description "Off Chip 1.5V Reference value.")
            )
        )
        (field
            (name CKMODE)
            (bit-offset 4)
            (bit-width 1)
            (access read-write)
            (description "Clock mode register")
            (value
                (value "0")
                (name "LPCKMODE")
                (description "Disable the clock between scans for LPMODE0. Set LPCKMODE to 0x1 while configuring the ADC. value.")
            )
            (value
                (value "1")
                (name "LLCKMODE")
                (description "Low Latency Clock Mode. When set, HFRC and the adc_clk will remain on while in functioning in LPMODE0. value.")
            )
        )
        (field
            (name LPMODE)
            (bit-offset 3)
            (bit-width 1)
            (access read-write)
            (description "Select power mode to enter between active scans.")
            (value
                (value "0")
                (name "MODE0")
                (description "Low Power Mode 0. Leaves the ADC fully powered between scans with minimum latency between a trigger event and sample data collection. value.")
            )
            (value
                (value "1")
                (name "MODE1")
                (description "Low Power Mode 1. Powers down all circuity and clocks associated with the ADC until the next trigger event. Between scans, the reference buffer requires up to 50us of delay from a scan trigger event before the conversion will commence while operating in this mode. value.")
            )
        )
        (field
            (name RPTEN)
            (bit-offset 2)
            (bit-width 1)
            (access read-write)
            (description "This bit enables Repeating Scan Mode.")
            (value
                (value "0")
                (name "SINGLE_SCAN")
                (description "In Single Scan Mode, the ADC will complete a single scan upon each trigger event. value.")
            )
            (value
                (value "1")
                (name "REPEATING_SCAN")
                (description "In Repeating Scan Mode, the ADC will complete it\'s first scan upon the initial trigger event and all subsequent scans will occur at regular intervals defined by the configuration programmed for the CTTMRA3 internal timer until the timer is disabled or the ADC is disabled. When disabling the ADC (setting ADCEN to \'0\'), the RPTEN bit should be cleared. value.")
            )
        )
        (field
            (name ADCEN)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit enables the ADC module. While the ADC is enabled, the ADCCFG and SLOT Configuration regsiter settings must remain stable and unchanged. All configuration register settings, slot configuration settings and window comparison settings should be written prior to setting the ADCEN bit to \'1\'.")
            (value
                (value "0")
                (name "DIS")
                (description "Disable the ADC module. value.")
            )
            (value
                (value "1")
                (name "EN")
                (description "Enable the ADC module. value.")
            )
        )
    )
    (register
        (name STAT)
        (offset 0x4)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x1)
        (description "ADC Power Status")
        (field
            (name PWDSTAT)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Indicates the power-status of the ADC.")
            (value
                (value "0")
                (name "ON")
                (description "Powered on. value.")
            )
            (value
                (value "1")
                (name "POWERED_DOWN")
                (description "ADC Low Power Mode 1. value.")
            )
        )
    )
    (register
        (name SWT)
        (offset 0x8)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xff)
        (description "Software trigger")
        (field
            (name SWT)
            (bit-offset 0)
            (bit-width 8)
            (access read-write)
            (description "Writing 0x37 to this register generates a software trigger.")
            (value
                (value "55")
                (name "GEN_SW_TRIGGER")
                (description "Writing this value generates a software trigger. value.")
            )
        )
    )
    (register
        (name SLCFG)
