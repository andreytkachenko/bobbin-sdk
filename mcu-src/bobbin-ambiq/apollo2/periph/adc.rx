            (description "Slot number associated with this FIFO data.")
        )
        (field
            (name COUNT)
            (bit-offset 20)
            (bit-width 8)
            (access read-write)
            (description "Number of valid entries in the ADC FIFO.")
        )
        (field
            (name DATA)
            (bit-offset 0)
            (bit-width 20)
            (access read-write)
            (description "Oldest data in the FIFO.")
        )
    )
