(peripheral
    (group-name VCOMP)
    (size 0x20)
    (access read-write)
    (description "Voltage Comparator")
    (register
        (name CFG)
        (offset 0x0)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xf0303)
        (description "Configuration Register")
        (field
            (name LVLSEL)
            (bit-offset 16)
            (bit-width 4)
            (access read-write)
            (description "When the reference input NSEL is set to NSEL_DAC, this bitfield selects the voltage level for the negative input to the comparator.")
            (value
                (value "0")
                (name "0P58V")
                (description "Set Reference input to 0.58 Volts. value.")
            )
            (value
                (value "1")
                (name "0P77V")
                (description "Set Reference input to 0.77 Volts. value.")
            )
            (value
                (value "2")
                (name "0P97V")
                (description "Set Reference input to 0.97 Volts. value.")
            )
            (value
                (value "3")
                (name "1P16V")
                (description "Set Reference input to 1.16 Volts. value.")
            )
            (value
                (value "4")
                (name "1P35V")
                (description "Set Reference input to 1.35 Volts. value.")
            )
            (value
                (value "5")
                (name "1P55V")
                (description "Set Reference input to 1.55 Volts. value.")
            )
            (value
                (value "6")
                (name "1P74V")
                (description "Set Reference input to 1.74 Volts. value.")
            )
            (value
                (value "7")
                (name "1P93V")
                (description "Set Reference input to 1.93 Volts. value.")
            )
            (value
                (value "8")
                (name "2P13V")
                (description "Set Reference input to 2.13 Volts. value.")
            )
            (value
                (value "9")
                (name "2P32V")
                (description "Set Reference input to 2.32 Volts. value.")
            )
            (value
                (value "10")
                (name "2P51V")
                (description "Set Reference input to 2.51 Volts. value.")
            )
            (value
                (value "11")
                (name "2P71V")
                (description "Set Reference input to 2.71 Volts. value.")
            )
            (value
                (value "12")
                (name "2P90V")
                (description "Set Reference input to 2.90 Volts. value.")
            )
            (value
                (value "13")
                (name "3P09V")
                (description "Set Reference input to 3.09 Volts. value.")
            )
            (value
                (value "14")
                (name "3P29V")
                (description "Set Reference input to 3.29 Volts. value.")
            )
            (value
                (value "15")
                (name "3P48V")
                (description "Set Reference input to 3.48 Volts. value.")
            )
        )
        (field
            (name NSEL)
            (bit-offset 8)
            (bit-width 2)
            (access read-write)
            (description "This bitfield selects the negative input to the comparator.")
            (value
                (value "0")
                (name "VREFEXT1")
                (description "Use external reference 1 for reference input. value.")
            )
            (value
                (value "1")
                (name "VREFEXT2")
                (description "Use external reference 2 for reference input. value.")
            )
            (value
                (value "2")
                (name "VREFEXT3")
                (description "Use external reference 3 for reference input. value.")
            )
            (value
                (value "3")
                (name "DAC")
                (description "Use DAC output selected by LVLSEL for reference input. value.")
            )
        )
        (field
            (name PSEL)
            (bit-offset 0)
            (bit-width 2)
            (access read-write)
            (description "This bitfield selects the positive input to the comparator.")
            (value
                (value "0")
                (name "VDDADJ")
                (description "Use VDDADJ for the positive input. value.")
            )
            (value
                (value "1")
                (name "VTEMP")
                (description "Use the temperature sensor output for the positive input. Note: If this channel is selected for PSEL, the bandap circuit required for temperature comparisons will automatically turn on. The bandgap circuit requires 11us to stabalize. value.")
            )
            (value
                (value "2")
                (name "VEXT1")
                (description "Use external voltage 0 for positive input. value.")
            )
            (value
                (value "3")
                (name "VEXT2")
                (description "Use external voltage 1 for positive input. value.")
            )
        )
    )
    (register
        (name STAT)
        (offset 0x4)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x3)
        (description "Status Register")
        (field
            (name PWDSTAT)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "This bit indicates the power down state of the voltage comparator.")
            (value
                (value "1")
                (name "POWERED_DOWN")
                (description "The voltage comparator is powered down. value.")
            )
        )
        (field
            (name CMPOUT)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit is 1 if the positive input of the comparator is greater than the negative input.")
            (value
                (value "0")
                (name "VOUT_LOW")
                (description "The negative input of the comparator is greater than the positive input. value.")
            )
            (value
                (value "1")
                (name "VOUT_HIGH")
                (description "The positive input of the comparator is greater than the negative input. value.")
            )
        )
    )
    (register
        (name PWDKEY)
        (offset 0x8)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0xffffffff)
        (description "Key Register for Powering Down the Voltage Comparator")
        (field
            (name PWDKEY)
            (bit-offset 0)
            (bit-width 32)
            (access read-write)
            (description "Key register value.")
            (value
                (value "55")
                (name "Key")
                (description "Key value.")
            )
        )
    )
    (register
        (name INTEN)
        (offset 0x200)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x3)
        (description "Voltage Comparator Interrupt registers: Enable")
        (field
            (name OUTHI)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout high interrupt.")
        )
        (field
            (name OUTLOW)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout low interrupt.")
        )
    )
    (register
        (name INTSTAT)
        (offset 0x204)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x3)
        (description "Voltage Comparator Interrupt registers: Status")
        (field
            (name OUTHI)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout high interrupt.")
        )
        (field
            (name OUTLOW)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout low interrupt.")
        )
    )
    (register
        (name INTCLR)
        (offset 0x208)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x3)
        (description "Voltage Comparator Interrupt registers: Clear")
        (field
            (name OUTHI)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout high interrupt.")
        )
        (field
            (name OUTLOW)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout low interrupt.")
        )
    )
    (register
        (name INTSET)
        (offset 0x20c)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x3)
        (description "Voltage Comparator Interrupt registers: Set")
        (field
            (name OUTHI)
            (bit-offset 1)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout high interrupt.")
        )
        (field
            (name OUTLOW)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "This bit is the vcompout low interrupt.")
        )
    )
)