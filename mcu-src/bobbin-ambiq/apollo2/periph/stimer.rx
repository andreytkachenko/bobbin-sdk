            (dim 4)
            (dim-increment 1)
            (bit-offset 0)
            (bit-width 1)
            (access read-write)
            (description "Selects whether capture is enabled for the specified capture register.")
            (value
                (value "0")
                (name "DISABLE")
                (description "Capture function disabled. value.")
            )
            (value
                (value "1")
                (name "ENABLE")
                (description "Capture function enabled. value.")
            )
        )
    )
    (register
