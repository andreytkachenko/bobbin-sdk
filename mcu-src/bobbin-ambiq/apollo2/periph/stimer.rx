    (register
        (name STMINTEN)
        (offset 0x300)
        (size 0x20)
        (access read-write)
        (reset-value 0x0)
        (reset-mask 0x1fff)
        (description "STIMER Interrupt registers: Enable")
        (field
            (name CAPTURE)
            (dim 4)
            (dim-increment 1)
            (bit-offset 9)
            (bit-width 1)
            (access read-write)
            (description "CAPTURE register n has grabbed the value in the counter")
            (value
