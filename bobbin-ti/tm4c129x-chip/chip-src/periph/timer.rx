(peripheral
    (group-name TIMER)
    (register
        (name CFG)
        (offset 0x0)
        (description "GPTM Configuration")
        (field
            (name CFG)
            (bit-offset 0)
            (bit-width 3)
            (description "GPTM Configuration")
            (value
                (value "0x0")
                (name "32_BIT_TIMER")
                (description "For a 16/32-bit timer, this value selects the 32-bit timer configuration")
            )
            (value
                (value "0x1")
                (name "32_BIT_RTC")
                (description "For a 16/32-bit timer, this value selects the 32-bit real-time clock (RTC) counter configuration")
            )
            (value
                (value "0x4")
                (name "16_BIT")
                (description "For a 16/32-bit timer, this value selects the 16-bit timer configuration")
            )
        )
    )
    (register
        (name TMR)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x4)
        (description "GPTM Timer n Mode")
        (field
            (name TMR)
            (bit-offset 0)
            (bit-width 2)
            (description "GPTM Timer n Mode")
            (value
                (value "0x1")
                (name "TAMR_1_SHOT")
                (description "One-Shot Timer mode")
            )
            (value
                (value "0x2")
                (name "TAMR_PERIOD")
                (description "Periodic Timer mode")
            )
            (value
                (value "0x3")
                (name "TAMR_CAP")
                (description "Capture mode")
            )
        )
        (field
            (name TCMR)
            (bit-offset 2)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode")
        )
        (field
            (name TAMS)
            (bit-offset 3)
            (bit-width 1)
            (description "GPTM Timer n Alternate Mode Select")
        )
        (field
            (name TCDIR)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM Timer n Count Direction")
        )
        (field
            (name TMIE)
            (bit-offset 5)
            (bit-width 1)
            (description "GPTM Timer n Match Interrupt Enable")
        )
        (field
            (name TWOT)
            (bit-offset 6)
            (bit-width 1)
            (description "GPTM Timer n Wait-on-Trigger")
        )
        (field
            (name TSNAPS)
            (bit-offset 7)
            (bit-width 1)
            (description "GPTM Timer n Snap-Shot Mode")
        )
        (field
            (name TILD)
            (bit-offset 8)
            (bit-width 1)
            (description "GPTM Timer n Interval Load Write")
        )
        (field
            (name TPWMIE)
            (bit-offset 9)
            (bit-width 1)
            (description "GPTM Timer n PWM Interrupt Enable")
        )
        (field
            (name TMRSU)
            (bit-offset 10)
            (bit-width 1)
            (description "GPTM Timer n Match Register Update")
        )
        (field
            (name TPLO)
            (bit-offset 11)
            (bit-width 1)
            (description "GPTM Timer n PWM Legacy Operation")
        )
        (field
            (name TCINTD)
            (bit-offset 12)
            (bit-width 1)
            (description "One-shot/Periodic Interrupt Disable")
        )
        (field
            (name TCACT)
            (bit-offset 13)
            (bit-width 3)
            (description "Timer Compare Action Select")
            (value
                (value "0x0")
                (name "TCACT_NONE")
                (description "Disable compare operations")
            )
            (value
                (value "0x1")
                (name "TCACT_TOGGLE")
                (description "Toggle State on Time-Out")
            )
            (value
                (value "0x2")
                (name "TCACT_CLRTO")
                (description "Clear CCP on Time-Out")
            )
            (value
                (value "0x3")
                (name "TCACT_SETTO")
                (description "Set CCP on Time-Out")
            )
            (value
                (value "0x4")
                (name "TCACT_SETTOGTO")
                (description "Set CCP immediately and toggle on Time-Out")
            )
            (value
                (value "0x5")
                (name "TCACT_CLRTOGTO")
                (description "Clear CCP immediately and toggle on Time-Out")
            )
            (value
                (value "0x6")
                (name "TCACT_SETCLRTO")
                (description "Set CCP immediately and clear on Time-Out")
            )
            (value
                (value "0x7")
                (name "TCACT_CLRSETTO")
                (description "Clear CCP immediately and set on Time-Out")
            )
        )
    )
    (register
        (name CTL)
        (offset 0xc)
        (description "GPTM Control")
        (field
            (name TEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (description "GPTM Timer n Enable")
        )
        (field
            (name TSTALL)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (description "GPTM Timer n Stall Enable")
        )
        (field
            (name TEVENT)
            (dim 2)
            (dim-increment 8)
            (bit-offset 2)
            (bit-width 2)
            (description "GPTM Timer n Event Mode")
            (value
                (value "0x0")
                (name "TAEVENT_POS")
                (description "Positive edge")
            )
            (value
                (value "0x1")
                (name "TAEVENT_NEG")
                (description "Negative edge")
            )
            (value
                (value "0x3")
                (name "TAEVENT_BOTH")
                (description "Both edges")
            )
        )
        (field
            (name RTCEN)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM RTC Stall Enable")
        )
        (field
            (name TOTE)
            (dim 2)
            (dim-increment 8)           
            (bit-offset 5)
            (bit-width 1)
            (description "GPTM Timer n Output Trigger Enable")
        )
        (field
            (name TPWML)
            (dim 2)
            (dim-increment 8)            
            (bit-offset 6)
            (bit-width 1)
            (description "GPTM Timer n PWM Output Level")
        )
    )
    (register
        (name SYNC)
        (offset 0x10)
        (description "GPTM Synchronize")
        (field
            (name SYNCT)
            (dim 8)
            (dim-increment 2)
            (bit-offset 0)
            (bit-width 2)
            (description "Synchronize GPTM Timer n")
            (value
                (value "0x0")
                (name "SYNCT0_NONE")
                (description "GPTM0 is not affected")
            )
            (value
                (value "0x1")
                (name "SYNCT0_TA")
                (description "A timeout event for Timer A of GPTMn is triggered")
            )
            (value
                (value "0x2")
                (name "SYNCT0_TB")
                (description "A timeout event for Timer B of GPTMn is triggered")
            )
            (value
                (value "0x3")
                (name "SYNCT0_TATB")
                (description "A timeout event for both Timer A and Timer B of GPTMn is triggered")
            )
        )
    )
    (register
        (name IMR)
        (offset 0x18)
        (description "GPTM Interrupt Mask")
        (field
            (name TTOIM)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (description "GPTM Timer n Time-Out Interrupt Mask")
        )
        (field
            (name CMIM)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode Match Interrupt Mask")
        )
        (field
            (name CEIM)
            (dim 2)
            (dim-increment 8)           
            (bit-offset 2)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode Event Interrupt Mask")
        )
        (field
            (name RTCIM)
            (bit-offset 3)
            (bit-width 1)
            (description "GPTM RTC Interrupt Mask")
        )
        (field
            (name TMIM)
            (dim 2)
            (dim-increment 8)            
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM Timer n Match Interrupt Mask")
        )
        (field
            (name DMAIM)
            (dim 2)
            (dim-increment 8)
            (bit-offset 5)
            (bit-width 1)
            (description "GPTM Timer n DMA Done Interrupt Mask")
        )
    )
    (register
        (name RIS)
        (offset 0x1c)
        (description "GPTM Raw Interrupt Status")
        (field
            (name TTORIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (description "GPTM Timer n Time-Out Raw Interrupt")
        )
        (field
            (name CMRIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode Match Raw Interrupt")
        )
        (field
            (name CERIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 2)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode Event Raw Interrupt")
        )
        (field
            (name RTCRIS)
            (bit-offset 3)
            (bit-width 1)
            (description "GPTM RTC Raw Interrupt")
        )
        (field
            (name TAMRIS)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM Timer A Match Raw Interrupt")
        )       
        (field
            (name DMARIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 5)
            (bit-width 1)
            (description "GPTM Timer n DMA Done Raw Interrupt Status")
        )
        (field
            (name TBMRIS)
            (bit-offset 11)
            (bit-width 1)
            (description "GPTM Timer B Match Raw Interrupt")
        )        
    )
    (register
        (name MIS)
        (offset 0x20)
        (description "GPTM Masked Interrupt Status")
        (field
            (name TTOMIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (description "GPTM Timer n Time-Out Masked Interrupt")
        )
        (field
            (name CMMIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode Match Masked Interrupt")
        )
        (field
            (name CEMIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 2)
            (bit-width 1)
            (description "GPTM Timer n Capture Mode Event Masked Interrupt")
        )
        (field
            (name RTCMIS)
            (bit-offset 3)
            (bit-width 1)
            (description "GPTM RTC Masked Interrupt")
        )
        (field
            (name TAMMIS)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM Timer A Match Masked Interrupt")
        )
        (field
            (name DMAMIS)
            (dim 2)
            (dim-increment 8)
            (bit-offset 5)
            (bit-width 1)
            (description "GPTM Timer n DMA Done Masked Interrupt")
        )
        (field
            (name TBMMIS)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM Timer B Match Masked Interrupt")
        )
    )
    (register
        (name ICR)
        (offset 0x24)
        (access write-only)
        (description "GPTM Interrupt Clear")
        (field
            (name TTOCINT)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (access write-only)
            (description "GPTM Timer n Time-Out Raw Interrupt")
        )
        (field
            (name CMCINT)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (access write-only)
            (description "GPTM Timer n Capture Mode Match Interrupt Clear")
        )
        (field
            (name CECINT)
            (dim 2)
            (dim-increment 8)
            (bit-offset 2)
            (bit-width 1)
            (access write-only)
            (description "GPTM Timer n Capture Mode Event Interrupt Clear")
        )
        (field
            (name RTCCINT)
            (bit-offset 3)
            (bit-width 1)
            (access write-only)
            (description "GPTM RTC Interrupt Clear")
        )
        (field
            (name TAMCINT)
            (bit-offset 4)
            (bit-width 1)
            (access write-only)
            (description "GPTM Timer A Match Interrupt Clear")
        )
        (field
            (name DMAINT)
            (dim 2)
            (dim-increment 8)
            (bit-offset 5)
            (bit-width 1)
            (access write-only)
            (description "GPTM Timer n DMA Done Interrupt Clear")
        )
        (field
            (name TBMCINT)
            (bit-offset 11)
            (bit-width 1)
            (access write-only)
            (description "GPTM Timer B Match Interrupt Clear")
        )        
    )
    (register
        (name TILR)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x28)
        (description "GPTM Timer n Interval Load")
        (field
            (name TILR)
            (bit-offset 0)
            (bit-width 32)
        )
    )
    (register
        (name TMTCHR)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x30)
        (description "GPTM Timer n Match")
        (field
            (name TMTCHR)
            (bit-offset 0)
            (bit-width 32)
        )
    )
    (register
        (name TPR)
        (dim 2)
        (dim-increment 0x4)        
        (offset 0x38)
        (description "GPTM Timer n Prescale")
        (field
            (name TPSR)
            (bit-offset 0)
            (bit-width 8)
            (description "GPTM Timer n Prescale")
        )
    )
    (register
        (name TPMR)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x40)
        (description "GPTM Timer n Prescale Match")
        (field
            (name TPSMR)
            (bit-offset 0)
            (bit-width 8)
            (description "GPTM Timer n Prescale Match")
        )
    )
    (register
        (name TR)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x48)
        (description "GPTM Timer n")
        (field
            (name TR)
            (bit-offset 0)
            (bit-width 32)
        )
    )
    (register
        (name TV)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x50)
        (description "GPTM Timer n Value")
        (field
            (name TV)
            (bit-offset 0)
            (bit-width 32)
        )
    )    
    (register
        (name RTCPD)
        (offset 0x58)
        (description "GPTM RTC Predivide")
        (field
            (name RTCPD)
            (bit-offset 0)
            (bit-width 16)
            (description "RTC Predivide Counter Value")
        )
    )
    (register
        (name TPS)
        (dim 2)
        (dim-increment 0x4)
        (offset 0x5c)
        (description "GPTM Timer n Prescale Snapshot")
        (field
            (name PSS)
            (bit-offset 0)
            (bit-width 16)
            (description "GPTM Timer n Prescaler Snapshot")
        )
    )
    (register
        (name DMAEV)
        (offset 0x6c)
        (description "GPTM DMA Event")
        (field
            (name TTODMAEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (description "GPTM n Time-Out Event DMA Trigger Enable")
        )
        (field
            (name CMDMAEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (description "GPTM n Capture Match Event DMA Trigger Enable")
        )
        (field
            (name CEDMAEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 2)
            (bit-width 1)
            (description "GPTM n Capture Event DMA Trigger Enable")
        )
        (field
            (name RTCDMAEN)
            (bit-offset 3)
            (bit-width 1)
            (description "GPTM RTC Match Event DMA Trigger Enable")
        )
        (field
            (name TMDMAEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM n Mode Match Event DMA Trigger Enable")
        )
    )
    (register
        (name ADCEV)
        (offset 0x70)
        (description "GPTM ADC Event")
        (field
            (name TTOADCEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 0)
            (bit-width 1)
            (description "GPTM n Time-Out Event ADC Trigger Enable")
        )
        (field
            (name CMADCEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 1)
            (bit-width 1)
            (description "GPTM n Capture Match Event ADC Trigger Enable")
        )
        (field
            (name CEADCEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 2)
            (bit-width 1)
            (description "GPTM Capture Event ADC Trigger Enable")
        )
        (field
            (name RTCADCEN)
            (bit-offset 3)
            (bit-width 1)
            (description "GPTM RTC Match Event ADC Trigger Enable")
        )
        (field
            (name TMADCEN)
            (dim 2)
            (dim-increment 8)
            (bit-offset 4)
            (bit-width 1)
            (description "GPTM n Mode Match Event ADC Trigger Enable")
        )
    )
    (register
        (name PP)
        (offset 0xfc0)
        (description "GPTM Peripheral Properties")
        (field
            (name SIZE)
            (bit-offset 0)
            (bit-width 4)
            (description "Count Size")
            (value
                (value "0x0")
                (name "SIZE_16")
                (description "Timer A and Timer B counters are 16 bits each with an 8-bit prescale counter")
            )
            (value
                (value "0x1")
                (name "SIZE_32")
                (description "Timer A and Timer B counters are 32 bits each with a 16-bit prescale counter")
            )
        )
        (field
            (name CHAIN)
            (bit-offset 4)
            (bit-width 1)
            (description "Chain with Other Timers")
        )
        (field
            (name SYNCCNT)
            (bit-offset 5)
            (bit-width 1)
            (description "Synchronize Start")
        )
        (field
            (name ALTCLK)
            (bit-offset 6)
            (bit-width 1)
            (description "Alternate Clock Source")
        )
    )
    (register
        (name CC)
        (offset 0xfc8)
        (description "GPTM Clock Configuration")
        (field
            (name ALTCLK)
            (bit-offset 0)
            (bit-width 1)
            (description "Alternate Clock Source")
        )
    )
)