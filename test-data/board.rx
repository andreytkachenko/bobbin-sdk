(board (name NUCLEO-L031)
    (device (name MCU))
    (device (name GPS))
    (device (name ARDUINO)
        (signal D0)
        (signal D1)
        (signal D2)
        (signal D3)
        (signal D4)
        (signal D5)
        (signal D6)
        (signal D7)
    )
    (device (name LED0) (signal IN))
    (device (name LED1) (signal IN))
    (device (name SW0) (signal OUT))
