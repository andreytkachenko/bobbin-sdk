        (MCU:OSC0 XTAL0)
        (MCU:PA0 GPS:UART_RX)
        (MCU:PA1 GPS:UART_TX)
        (MCU:PA4 LED0:IN)
        (MCU:PA5 LED1:IN)
        (MCU:PA3 SW0:OUT)
        (MCU:PA0 ARDUINO:D0)
        (MCU:PA1 ARDUINO:D1)
        (MCU:PA2 ARDUINO:D2)
        (MCU:PA3 ARDUINO:D3)
        (MCU:PA4 ARDUINO:D3)
        (MCU:PA5 ARDUINO:D3)
    )
)